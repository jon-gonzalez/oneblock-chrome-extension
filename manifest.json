{
  "manifest_version": 2,

  "name": "1Block Authentication Plugin",
  "short_name": "1Block",
  "description": "This extension is a complete 1Block client for the browser",
  "version": "1.3",

  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "storage",
   "background"
   ]
}
