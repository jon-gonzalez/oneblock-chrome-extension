{
  "manifest_version": 2,

  "name": "1Block Authentication Plugin",
  "description": "This extension is a complete 1Block client for the browser",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "clipboardRead",
   "clipboardWrite",
   "cookies",
   "sessions",
   "storage"
   ]
}
